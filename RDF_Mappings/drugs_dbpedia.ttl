@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#Drug>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/drugs_dbpedia.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_ID}";
		rr:class corvid-19:Drug
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:drugLabel;
		rr:objectMap [
			rml:reference "DrugName";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:drugBankID;
		rr:objectMap [
			rml:reference "http://corvid-19.tib.eu/vocab/{DrugBankID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{DBpediaURL}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:externalLink;
		rr:objectMap [
			rr:template "https://www.drugbank.ca/drugs/{DrugBankID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://bio2rdf.org/drugbank:{DrugBankID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://wifo5-04.informatik.uni-mannheim.de/drugbank/resource/drugs/{DrugBankID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:provenance;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/SourceName/UMLS"
		]
	].

