@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#DrugToxicity>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/drugs_data/drugs_toxicities.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_ID}_hasToxicity_{toxicity_CUI}";
		rr:class corvid-19:DrugToxicity
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:drugBankID;
		rr:objectMap [
			rml:reference "http://corvid-19.tib.eu/vocab/{drug_id}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:toxicity_label;
		rr:objectMap [
			rml:reference "toxicity_Label";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasToxicityCUI;
		rr:objectMap [
			rml:reference "http://corvid-19.tib.eu/vocab/{toxicity_CUI}";

		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasDrugCUI;
		rr:objectMap [
			rml:reference "http://corvid-19.tib.eu/vocab/{CUI_ID}";
		]
	].

