		]
	].

<#DrugDrugInteractionNS>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/drugs_data/Unsymmetric_DDI_corpus.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/vocab/{CUI_1}_interactsWith_{CUI_2}";
		rr:class covid-19:DrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasDrug1CUI;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/vocab/{CUI_1}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasDrug2CUI;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/vocab/{CUI_2}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:precipitant;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/vocab/{PrecipitantDrugID}"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:object;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/vocab/{ObjectDrugID}"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:interaction;
		rr:objectMap [ 
			rml:reference "Interaction";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:Effect;
		rr:objectMap [
  			rr:parentTriplesMap <#Effect>;
		];
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:provenance;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/SourceName/UMLS";
		]
	].

