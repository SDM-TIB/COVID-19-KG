@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<#Adverse_Event>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/drugs_data/Symmetric_DDI_corpus.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/vocab/{CUI_AdverseEvent}";
		rr:class covid-19:Adverse_Event
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:impact;
		rr:objectMap [ 
			rml:reference "Impact"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:Adverse_Event;
		rr:objectMap [ 
			rml:reference "Adverse_Event"; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:subClassOf;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/vocab/{CUI_AdverseEvent}";
		]
	].

<#Effect>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/drugs_data/Unsymmetric_DDI_corpus.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/vocab/{CUI_AdverseEvent}";
