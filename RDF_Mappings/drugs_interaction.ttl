@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#Effect>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/drugs_interaction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/Effect/{effect}";
		rr:class corvid-19:Effect
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:effectLabel;
		rr:objectMap [
			rml:reference "effect"
		]
	].

<#Impact>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/drugs_interaction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/Impact/{property}";
		rr:class corvid-19:Impact
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:impactLabel;
		rr:objectMap [ 
			rml:reference "property" 
		]
	].	

<#DrugDrugInteraction>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/drugs_interaction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_1}_interactsWith_{CUI_2}";
		rr:class corvid-19:DrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasDrug1CUI;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{CUI_1}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasDrug2CUI;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{CUI_2}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:affects;
		rr:objectMap [ 
			rr:template "http://corvid-19.tib.eu/vocab/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:isAffected;
		rr:objectMap [ 
			rr:template "http://corvid-19.tib.eu/vocab/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasEffect;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/Effect/{effect}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasImpact;
		rr:objectMap [
			rr:template "hhttp://corvid-19.tib.eu/Impact/{property}"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate corvid-19:provenance;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/SourceName/UMLS"
		]
	].

