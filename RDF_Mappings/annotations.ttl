@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#Annotation>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/annotations.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{cord_uid}_has_{CUI}";
		rr:class corvid-19:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:annotates;
		rr:objectMap [ 
			rr:template "http://corvid-19.tib.eu/Publication/{cord_uid}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasAnnotation;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{CUI}"
		]
	].	
