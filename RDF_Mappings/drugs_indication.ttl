@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#DrugIndication>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/drugs_indications.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_ID}_isPrescribedFor_{indication_Id}";
		rr:class corvid-19:DrugIndication
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:drugLabel;
		rr:objectMap [
			rml:reference "DrugName";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:drugId;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{DrugId}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasDrugCUI;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{CUI_Id}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:hasIndicationCUI;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/vocab/{indication_Id}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:indication_Label;
		rr:objectMap [
			rml:reference "indication_name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{Dbpedia_url}";
		]
	].
