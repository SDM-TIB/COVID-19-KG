@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix corvid-19: <http://corvid-19.tib.eu/vocab/> .

<#Concept>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/diseases_syndromes.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_ID}";
		rr:class corvid-19:Concept
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:concept_Label;
		rr:objectMap [ 
			rml:reference "Concept" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:provenance;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/SourceName/UMLS"
		]
	].

<#Substance>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF_Mappings/substances.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://corvid-19.tib.eu/vocab/{CUI_ID}";
		rr:class corvid-19:Substance
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:substance_Label;
		rr:objectMap [ 
			rml:reference "Substance" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:provenance;
		rr:objectMap [
			rr:template "http://corvid-19.tib.eu/SourceName/UMLS"
		]
	].
