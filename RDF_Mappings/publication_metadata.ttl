	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:full_text_file;
		rr:objectMap [ 
			rml:reference "full_text_file" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate corvid-19:url;
		rr:objectMap [ 
