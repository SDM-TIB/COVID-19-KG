@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<#COVID_Annotation>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/covid_data/all_covid_ann.csv";
	rml:referenceFormulation ql:CSV 
	];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/vocab/{cord_uid}_has_{covid_id}";
		rr:class covid-19:COVID_Annotation
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:annotates;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/Publication/{cord_uid}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasAnnotation;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/vocab/{covid_id}"
		]
	].	
