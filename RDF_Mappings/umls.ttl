@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<#CUI_UMLS>
	rml:logicalSource [ rml:source "/home/anery/Desktop/Covid_Data/RDF/drugs_data/MultiLang_with_covid.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/vocab/{CUI_ID}";
		rr:class covid-19:CUI_UMLS
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:label;
		rr:objectMap [ 
			rml:reference "Label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:provenance;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/SourceName/UMLS"
		]
	].
