@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<ConceptAnnotation>
     a rr:TriplesMap;
	rml:logicalSource [ rml:source "/data/pub/entities.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/hasAnnotation/{subject}_{predicate}_{object}";
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:annotates;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/Publication/{subject}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasSemanticAnnotation;
		rr:objectMap [
			rr:parentTripleMap <SemanticObjectAnnotation> 	
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:type;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/vocab/{predicate}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasConfidenceScore;
		rr:objectMap [
			rml:reference "score";
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/SourceName/Semrep";
		]
	].

<SemanticObjectAnnotation>
a rr:TriplesMap;
	rml:logicalSource [ rml:source "/data/pub/CORD19_entities.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/SemanticAnnotation/{object}_istype_{sem_type}";
		rr:class covid-19:SemanticAnnotation
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasSemanticType;
		rr:objectMap [ 
			rml:reference "sem_type";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasCUIAnnotation;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/Annotation/{object}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/SourceName/Semrep";
		]
	].

